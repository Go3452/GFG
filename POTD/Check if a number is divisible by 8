class Solution{
    int DivisibleByEight(String s){
      int n=s.length();
      if(n<=3)
      {
       int num=Integer.parseInt(s);
       if(num%8==0)
        return 1;
       return -1;
      }
      else
      {
       String S=s.substring(n-3,n);
       int num=Integer.parseInt(S);
       if(num%8==0)
        return 1;
       return -1;
      }
    }
}
