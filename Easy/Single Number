class Solution {
    int getSingle(int arr[]) {
        Arrays.sort(arr);
        int freq=1,num=arr[0];
        int n=arr.length;
        for(int i=1;i<n;i++)
        {
         if(num==arr[i])
          freq++;
         else
         {
          if(freq%2==1)
          {
           return num;
          }
          freq=1;
          num=arr[i];
         }
        }
        return arr[n-1];
    }
}
