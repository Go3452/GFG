class Solution {
    static ArrayList<Integer> increment(ArrayList<Integer> arr , int N) {
        int carry=1;
        for(int i=N-1;i>=0;i--)
        {
         int sum=arr.get(i)+carry;
         if(sum>9)
         {
          carry=sum/10;
          sum%=10;
          
         }
         else
          carry=0;
         arr.set(i,sum);
        }
        if(carry==1)
         arr.add(0,1);
        return arr;
    }
};
