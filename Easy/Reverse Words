class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
     String []arr = S.split("[.]");
     int n=arr.length;
     String ans="";
     for(int i=n-1;i>=0;i--)
     {
      if(i==0)
       ans+=arr[i];
      else
       ans+=arr[i]+".";
     }
     return ans;
    }
}
