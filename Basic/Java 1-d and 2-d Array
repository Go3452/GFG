class Complete
{
    public static ArrayList<Integer> array(int a[][], int b[], int n)
    {
     ArrayList<Integer> arr = new ArrayList<Integer>();
     int sum=0,max=0;
     for(int i=0;i<n;i++)
     {
      if(max<b[i])
       max=b[i];
      sum+=a[i][i];
     }
     arr.add(sum);
     arr.add(max);
     return arr;
    }
}
